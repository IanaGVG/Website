    "build": "npm run prod:css && zola build && gh-pages -d public"
  },
  "author": "Iana Garbuzova",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.7",
    "tailwindcss": "^3.3.6"
  },
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "gh-pages": "^5.0.0",
    "postcss-cli": "^11.0.0"
  }
}
